local UpdateFOV2 = function ()
    if (not Circle2) then
        return Circle2
    end
    Circle2.Visible = Settings.FOV.AimAssist["Visible"]
    Circle2.Radius = FOVTOUSEC * 3
    Circle2.Position = Vector2.new(Mouse.X, Mouse.Y + (game:GetService("GuiService"):GetGuiInset().Y))
    if gunEquipedName ~= nil and Settings.GunFOV.Enabled == true then
        if gunEquipedName == "[Revolver]" then
            if Settings.GunFOV.Revolver[1] == true then
                FOVTOUSEC = Settings.GunFOV.Revolver[2]
            end
        elseif gunEquipedName == "[TacticalShotgun]" then
            if Settings.GunFOV.TacticalShotgun[1] == true then
                FOVTOUSEC = Settings.GunFOV.TacticalShotgun[2]
            end
        elseif gunEquipedName == "[Silencer]" then
            if Settings.GunFOV.Silencer[1] == true then
                FOVTOUSEC = Settings.GunFOV.Silencer[2]
            end
        elseif gunEquipedName == "[SMG]" then
            if Settings.GunFOV.SMG[1] == true then
                FOVTOUSEC = Settings.GunFOV.SMG[2]
            end
        elseif gunEquipedName == "[Double-Barrel SG]" then
            if Settings.GunFOV["Double-Barrel SG"][1] == true then
                FOVTOUSEC = Settings.GunFOV["Double-Barrel SG"][2]
            end
        elseif gunEquipedName == "[Shotgun]" then
            if Settings.GunFOV.Shotgun[1] == true then
                FOVTOUSEC = Settings.GunFOV.Shotgun[2]
            end
        elseif gunEquipedName == "[AR]" then
            if Settings.GunFOV.AR[1] == true then
                FOVTOUSEC = Settings.GunFOV.AR[2]
            end
        elseif gunEquipedName == "[AK47]" then
            if Settings.GunFOV.AK47[1] == true then
                FOVTOUSEC = Settings.GunFOV.AK47[2]
            end
        elseif gunEquipedName == "[Rifle]" then
            if Settings.GunFOV.Rifle[1] == true then
                FOVTOUSEC = Settings.GunFOV.Rifle[2]
            end
        elseif gunEquipedName == "[AUG]" then
            if Settings.GunFOV.AUG[1] == true then
                FOVTOUSEC = Settings.GunFOV.AUG[2]
            end
        elseif gunEquipedName == "[Drumgun]" then
            if Settings.GunFOV.Drumgun[1] == true then
                FOVTOUSEC = Settings.GunFOV.Drumgun[2]
            end
        elseif gunEquipedName == "[Flamethrower]" then
            if Settings.GunFOV.Flamethrower[1] == true then
                FOVTOUSEC = Settings.GunFOV.Flamethrower[2]
            end
        elseif gunEquipedName == "[SilencerAR]" then
            if Settings.GunFOV.SilencerAR[1] == true then
                FOVTOUSEC = Settings.GunFOV.SilencerAR[2]
            end
        elseif gunEquipedName == "[P90]" then
            if Settings.GunFOV.P90[1] == true then
                FOVTOUSEC = Settings.GunFOV.P90[2]
            end
        elseif gunEquipedName == "[Glock]" then
            if Settings.GunFOV.Glock[1] == true then
                FOVTOUSEC = Settings.GunFOV.Glock[2]
            end
        end
    elseif gunEquipedName == nil or Settings.GunFOV.Enabled == false and Settings.Commands.Enabled == false then
        FOVTOUSEC = Settings.FOV.AimAssist.Radius
    end
    Client.Chatted:Connect(function(Commands)
        if Settings.Commands.Enabled == true then
            local AimCommands = Settings.Commands.AimAssist
            local SilentCommands = Settings.Commands.Silent
            local msg = string.split(Commands, " ")
            if msg[1] == SilentCommands.FovSize and msg[2] ~= nil then
                FovToUse = msg[2]
            elseif msg[1] == AimCommands.FovSize and msg[2] ~= nil then
                FOVTOUSEC = msg[2]
            end
        end
    end)
    if Settings.GunOptimizations.Enabled == true then
        if Settings.GunOptimizations.Enabled == true then
            if Prey then
                Range = (Client.Character.HumanoidRootPart.Position - Prey.Character.HumanoidRootPart.Position).Magnitude
                if gunEquipedName == "[Double-Barrel SG]" then
                    if Settings.GunOptimizations["Double-Barrel SG"].FOV == true and Settings.GunOptimizations["Double-Barrel SG"].Pred == true then
                        if Range < Settings.GunOptimizations["Double-Barrel SG"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Double-Barrel SG"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].CloseDetection and Range < Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Double-Barrel SG"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Double-Barrel SG"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Double-Barrel SG"].FOV == false and Settings.GunOptimizations["Double-Barrel SG"].Pred == true then
                        if Range < Settings.GunOptimizations["Double-Barrel SG"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].CloseDetection and Range < Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].FarPrediction
