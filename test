                            Settings.Silent.Pred = Settings.GunOptimizations["Silencer"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Silencer"].CloseDetection and Range < Settings.GunOptimizations["Silencer"].FarDetection then
