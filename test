        ['ReCalculation_Interval'] = 0.1 --// Time Before recalculating velocity // --
        ['Velocity_Changer'] = { -- // This is another type of resolver that just changes the player your locked onto Velocity // --
            ['Enabled'] = true, -- // Whether its enabled or Disabled // --
            X = 0 -- // What you want the X axis to be so basically side to side. // --
            Y = 0 -- // What you want the Y axis to be so basically Up and down // --
