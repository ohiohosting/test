local Players, Client, Mouse, RS, Camera, Settings, Uisf, FovToUse, Prey =
    game:GetService("Players"),
    game:GetService("Players").LocalPlayer,
    game:GetService("Players").LocalPlayer:GetMouse(),
    game:GetService("RunService"),
    game.Workspace.CurrentCamera,
    getgenv().Settings,
    game:GetService("UserInputService"),
    Settings.FOV.Silent.Radius

local gunEquipedName = nil
local PointToAim, PointToAimP
local PartToUse = Settings.Silent.Part
local PPred
local UseMove = Settings.Prey_Resolving.MoveDirection_Resolver
local cplayer
local canC = false
local PartToUseC = Settings.AimAssist.Part
local FOVTOUSEC = Settings.FOV.AimAssist.Radius

local Guns = {
    "[Double-Barrel SG]",
    "[Revolver]",
    "[SMG]",
    "[Shotgun]",
    "[TacticalShotgun]",
    "[Silencer]",
    "[AR]",
    "[AK47]",
    "[Rifle]",
    "[AUG]",
    "[Drumgun]",
    "[Flamethrower]",
    "[SilencerAR]",
    "[P90]",
    "[Glock]",
} 
local PartsS = {"Head", "UpperTorso", "LowerTorso", "HumanoidRootPart", "RightFoot", "LeftFoot", "RightArm", "LeftArm",}
local GonnaHit = false
local DistanceFromTheNigga = nil
local Circle = Drawing.new("Circle")
Circle.Color = Color3.new(1,1,1)
Circle.Thickness = 1
local Circle2 = Drawing.new("Circle")
Circle2.Color = Color3.new(1,1,1)
Circle2.Thickness = 1

local UpdateFOV = function ()
    if (not Circle) then
        return Circle
    end
    Circle.Visible = Settings.FOV.Silent["Visible"]
    Circle.Radius = FovToUse * 3
    Circle.Position = Vector2.new(Mouse.X, Mouse.Y + (game:GetService("GuiService"):GetGuiInset().Y))
    if gunEquipedName ~= nil and Settings.GunFOV.Enabled == true then
        if gunEquipedName == "[Revolver]" then
            if Settings.GunFOV.Revolver[1] == true then
                FovToUse = Settings.GunFOV.Revolver[2]
            end
        elseif gunEquipedName == "[TacticalShotgun]" then
            if Settings.GunFOV.TacticalShotgun[1] == true then
                FovToUse = Settings.GunFOV.TacticalShotgun[2]
            end
        elseif gunEquipedName == "[Silencer]" then
            if Settings.GunFOV.Silencer[1] == true then
                FovToUse = Settings.GunFOV.Silencer[2]
            end
        elseif gunEquipedName == "[SMG]" then
            if Settings.GunFOV.SMG[1] == true then
                FovToUse = Settings.GunFOV.SMG[2]
            end
        elseif gunEquipedName == "[Double-Barrel SG]" then
            if Settings.GunFOV["Double-Barrel SG"][1] == true then
                FovToUse = Settings.GunFOV["Double-Barrel SG"][2]
            end
        elseif gunEquipedName == "[Shotgun]" then
            if Settings.GunFOV.Shotgun[1] == true then
                FovToUse = Settings.GunFOV.Shotgun[2]
            end
        elseif gunEquipedName == "[AR]" then
            if Settings.GunFOV.AR[1] == true then
                FovToUse = Settings.GunFOV.AR[2]
            end
        elseif gunEquipedName == "[AK47]" then
            if Settings.GunFOV.AK47[1] == true then
                FovToUse = Settings.GunFOV.AK47[2]
            end
        elseif gunEquipedName == "[Rifle]" then
            if Settings.GunFOV.Rifle[1] == true then
                FovToUse = Settings.GunFOV.Rifle[2]
            end
        elseif gunEquipedName == "[AUG]" then
            if Settings.GunFOV.AUG[1] == true then
                FovToUse = Settings.GunFOV.AUG[2]
            end
        elseif gunEquipedName == "[Drumgun]" then
            if Settings.GunFOV.Drumgun[1] == true then
                FovToUse = Settings.GunFOV.Drumgun[2]
            end
        elseif gunEquipedName == "[Flamethrower]" then
            if Settings.GunFOV.Flamethrower[1] == true then
                FovToUse = Settings.GunFOV.Flamethrower[2]
            end
        elseif gunEquipedName == "[SilencerAR]" then
            if Settings.GunFOV.SilencerAR[1] == true then
                FovToUse = Settings.GunFOV.SilencerAR[2]
            end
        elseif gunEquipedName == "[P90]" then
            if Settings.GunFOV.P90[1] == true then
                FovToUse = Settings.GunFOV.P90[2]
            end
        elseif gunEquipedName == "[Glock]" then
            if Settings.GunFOV.Glock[1] == true then
                FovToUse = Settings.GunFOV.Glock[2]
            end
        end
    elseif gunEquipedName == nil or Settings.GunFOV.Enabled == false and Settings.Commands.Enabled == false then
        FovToUse = Settings.FOV.Silent.Radius
    end
    Client.Chatted:Connect(function(Commands)
        if Settings.Commands.Enabled == true then
            local AimCommands = Settings.Commands.AimAssist
            local SilentCommands = Settings.Commands.Silent
            local msg = string.split(Commands, " ")
            if msg[1] == SilentCommands.FovSize and msg[2] ~= nil then
                FovToUse = msg[2]
            elseif msg[1] == AimCommands.FovSize and msg[2] ~= nil then
                Settings.FOV.AimAssist.Radius = msg[2]
            end
        end
    end)
    if Settings.GunOptimizations.Enabled == true then
        if Settings.GunOptimizations.Enabled == true then
            if Prey then
                Range = (Client.Character.HumanoidRootPart.Position - Prey.Character.HumanoidRootPart.Position).Magnitude
                if gunEquipedName == "[Double-Barrel SG]" then
                    if Settings.GunOptimizations["Double-Barrel SG"].FOV == true and Settings.GunOptimizations["Double-Barrel SG"].Pred == true then
                        if Range < Settings.GunOptimizations["Double-Barrel SG"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].CloseDetection and Range < Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Double-Barrel SG"].FOV == false and Settings.GunOptimizations["Double-Barrel SG"].Pred == true then
                        if Range < Settings.GunOptimizations["Double-Barrel SG"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].CloseDetection and Range < Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Double-Barrel SG"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Double-Barrel SG"].FOV == true and Settings.GunOptimizations["Double-Barrel SG"].Pred == false then
                        if Range < Settings.GunOptimizations["Double-Barrel SG"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].CloseFov
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].CloseDetection and Range < Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].MidFov
                        elseif Range > Settings.GunOptimizations["Double-Barrel SG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Double-Barrel SG"].FarFov
                        end
                    end
                elseif gunEquipedName == "[Revolver]" then
                    if Settings.GunOptimizations["Revolver"].FOV == true and Settings.GunOptimizations["Revolver"].Pred == true then
                        if Range < Settings.GunOptimizations["Revolver"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Revolver"].CloseDetection and Range < Settings.GunOptimizations["Revolver"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Revolver"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Revolver"].FOV == false and Settings.GunOptimizations["Revolver"].Pred == true then
                        if Range < Settings.GunOptimizations["Revolver"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Revolver"].CloseDetection and Range < Settings.GunOptimizations["Revolver"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Revolver"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Revolver"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Revolver"].FOV == true and Settings.GunOptimizations["Revolver"].Pred == false then
                        if Range < Settings.GunOptimizations["Revolver"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].CloseFov
                        elseif Range > Settings.GunOptimizations["Revolver"].CloseDetection and Range < Settings.GunOptimizations["Revolver"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].MidFov
                        elseif Range > Settings.GunOptimizations["Revolver"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Revolver"].FarFov
                        end
                    end
                elseif gunEquipedName == "[SMG]" then
                    if Settings.GunOptimizations["SMG"].FOV == true and Settings.GunOptimizations["SMG"].Pred == true then
                        if Range < Settings.GunOptimizations["SMG"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["SMG"].CloseDetection and Range < Settings.GunOptimizations["SMG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["SMG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["SMG"].FOV == false and Settings.GunOptimizations["SMG"].Pred == true then
                        if Range < Settings.GunOptimizations["SMG"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["SMG"].CloseDetection and Range < Settings.GunOptimizations["SMG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].MidPrediction
                        elseif Range > Settings.GunOptimizations["SMG"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["SMG"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["SMG"].FOV == true and Settings.GunOptimizations["SMG"].Pred == false then
                        if Range < Settings.GunOptimizations["SMG"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].CloseFov
                        elseif Range > Settings.GunOptimizations["SMG"].CloseDetection and Range < Settings.GunOptimizations["SMG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].MidFov
                        elseif Range > Settings.GunOptimizations["SMG"].FarDetection then
                            FovToUse = Settings.GunOptimizations["SMG"].FarFov
                        end
                    end
                elseif gunEquipedName == "[Shotgun]" then
                    if Settings.GunOptimizations["Shotgun"].FOV == true and Settings.GunOptimizations["Shotgun"].Pred == true then
                        if Range < Settings.GunOptimizations["Shotgun"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Shotgun"].CloseDetection and Range < Settings.GunOptimizations["Shotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Shotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Shotgun"].FOV == false and Settings.GunOptimizations["Shotgun"].Pred == true then
                        if Range < Settings.GunOptimizations["Shotgun"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Shotgun"].CloseDetection and Range < Settings.GunOptimizations["Shotgun"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Shotgun"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Shotgun"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Shotgun"].FOV == true and Settings.GunOptimizations["Shotgun"].Pred == false then
                        if Range < Settings.GunOptimizations["Shotgun"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].CloseFov
                        elseif Range > Settings.GunOptimizations["Shotgun"].CloseDetection and Range < Settings.GunOptimizations["Shotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].MidFov
                        elseif Range > Settings.GunOptimizations["Shotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["Shotgun"].FarFov
                        end
                    end
                elseif gunEquipedName == "[TacticalShotgun]" then
                    if Settings.GunOptimizations["TacticalShotgun"].FOV == true and Settings.GunOptimizations["TacticalShotgun"].Pred == true then
                        if Range < Settings.GunOptimizations["TacticalShotgun"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["TacticalShotgun"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].CloseDetection and Range < Settings.GunOptimizations["TacticalShotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["TacticalShotgun"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].MidPrediction
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["TacticalShotgun"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["TacticalShotgun"].FOV == false and Settings.GunOptimizations["TacticalShotgun"].Pred == true then
                        if Range < Settings.GunOptimizations["TacticalShotgun"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].CloseDetection and Range < Settings.GunOptimizations["TacticalShotgun"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].MidPrediction
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["TacticalShotgun"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["TacticalShotgun"].FOV == true and Settings.GunOptimizations["TacticalShotgun"].Pred == false then
                        if Range < Settings.GunOptimizations["TacticalShotgun"].CloseDetection then
                            FovToUse = Settings.GunOptimizations["TacticalShotgun"].CloseFov
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].CloseDetection and Range < Settings.GunOptimizations["TacticalShotgun"].FarDetection then
                            FovToUse = Settings.GunOptimizations["TacticalShotgun"].MidFov
                        elseif Range > Settings.GunOptimizations["TacticalShotgun"].FarDetection then
