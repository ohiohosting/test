

if not game:IsLoaded() then
    game.Loaded:Wait()
end

getgenv().Settings = {
    ['Silent'] = {
        ['Enabled'] = true, -- // WHether the silentaim is enabled or disabled // --
        ['Mode'] = "Mouse" -- Mouse or Target
        ['Part'] = "HumanoidRootPart", --// What Part to use // --
        ['Pred'] = 0.119, --// Prediction how much the camera moves additionally // --
        ['UseKeybindToDisable'] = false,
        ['Keybind'] = "k", -- // Keybind to Disable or Enable Script //--
        ['ClosestPart'] = true, --// Gets the closestPart to your mouse // --
        ['ClosestPoint'] = false, -- not done
        ['DisableOnDeath'] = true, -- DIsables on Targets Death // --
        ['UseHitChance'] = false, -- turn to false if you want it to always hit
        ['HitChance'] = 100 -- Max 100 dont even try to go higher it wont do anything
    },
    ['AimAssist'] = {
        ['Enable'] = true, -- // Whether the Aim Assist is enabled or not // --
        ['AttachOnSilent'] = true, -- // this makes the camlock attach when the silentaim attatches //-- 
        ['Pred'] = 0.123, -- // Prediction how much the camera moves additionally // --
        ['Smoothness'] = 0.1 -- // How smooth the camlock is //--
        ['Easing'] = "Sine", -- // THeres more info for this on roblox dev forums but this is basically how the camera lock feels I gues for the x axis // --
