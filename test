                        if Range < Settings.GunOptimizations["SilencerAR"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["SilencerAR"].CloseFov
                        elseif Range > Settings.GunOptimizations["SilencerAR"].CloseDetection and Range < Settings.GunOptimizations["SilencerAR"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["SilencerAR"].MidFov
                        elseif Range > Settings.GunOptimizations["SilencerAR"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["SilencerAR"].FarFov
                        end
                    end
                elseif gunEquipedName == "[P90]" then
                    if Settings.GunOptimizations["P90"].FOV == true and Settings.GunOptimizations["P90"].Pred == true then
                        if Range < Settings.GunOptimizations["P90"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["P90"].CloseDetection and Range < Settings.GunOptimizations["P90"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].MidPrediction
                        elseif Range > Settings.GunOptimizations["P90"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["P90"].FOV == false and Settings.GunOptimizations["P90"].Pred == true then
                        if Range < Settings.GunOptimizations["P90"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["P90"].CloseDetection and Range < Settings.GunOptimizations["P90"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].MidPrediction
                        elseif Range > Settings.GunOptimizations["P90"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["P90"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["P90"].FOV == true and Settings.GunOptimizations["P90"].Pred == false then
                        if Range < Settings.GunOptimizations["P90"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].CloseFov
                        elseif Range > Settings.GunOptimizations["P90"].CloseDetection and Range < Settings.GunOptimizations["P90"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].MidFov
                        elseif Range > Settings.GunOptimizations["P90"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["P90"].FarFov
                        end
                    end
                elseif gunEquipedName == "[Glock]" then
                    if Settings.GunOptimizations["Glock"].FOV == true and Settings.GunOptimizations["Glock"].Pred == true then
                        if Range < Settings.GunOptimizations["Glock"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].CloseFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Glock"].CloseDetection and Range < Settings.GunOptimizations["Glock"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].MidFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Glock"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].FarFov
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Glock"].FOV == false and Settings.GunOptimizations["Glock"].Pred == true then
                        if Range < Settings.GunOptimizations["Glock"].CloseDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].ClosePrediction
                        elseif Range > Settings.GunOptimizations["Glock"].CloseDetection and Range < Settings.GunOptimizations["Glock"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].MidPrediction
                        elseif Range > Settings.GunOptimizations["Glock"].FarDetection then
                            Settings.Silent.Pred = Settings.GunOptimizations["Glock"].FarPrediction
                        end
                    elseif Settings.GunOptimizations["Glock"].FOV == true and Settings.GunOptimizations["Glock"].Pred == false then
                        if Range < Settings.GunOptimizations["Glock"].CloseDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].CloseFov
                        elseif Range > Settings.GunOptimizations["Glock"].CloseDetection and Range < Settings.GunOptimizations["Glock"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].MidFov
                        elseif Range > Settings.GunOptimizations["Glock"].FarDetection then
                            FOVTOUSEC = Settings.GunOptimizations["Glock"].FarFov
                        end
                    end
                end
            end
        end
    end
    return Circle
end


RS.Heartbeat:Connect(function()
    UpdateFOV()
    UpdateFOV2()
end)

local ClosestPlrFromMouse = function()
    local Target, Closest = nil, 1/0
 
    for _ ,v in pairs(Players:GetPlayers()) do
        if (v.Character and v ~= Client and v.Character:FindFirstChild("HumanoidRootPart")) then
            local Position, OnScreen = Camera:WorldToScreenPoint(v.Character.HumanoidRootPart.Position)
            local Distance = (Vector2.new(Position.X, Position.Y) - Vector2.new(Mouse.X, Mouse.Y)).Magnitude

            if (Circle.Radius > Distance and Distance < Closest and OnScreen) then
                Closest = Distance
                Target = v
            end
        end
    end
    return Target
end



local FilterObjs = function (Object)
    if string.find(Object.Name, "Gun") then
        return
    end
    if table.find({"Part", "MeshPart", "BasePart"}, Object.ClassName) then
        return true
    end
end

local GetClosestBodyPart = function (character)
    local ClosestDistance
    local BodyPart
    if (character and character:GetChildren()) then
        for _,  x in next, character:GetChildren() do
            local IsOnScreen = camera:WorldToScreenPoint(character.HumanoidRootPart.Position)
            if FilterObjs(x) and IsOnScreen then
                local Distance = (Vector2.new(IsOnScreen.X, IsOnScreen.Y) - Vector2.new(mouse.X, mouse.Y)).Magnitude
                if Distance <= FovToUse *3 then
                    ClosestDistance = Distance
                    BodyPart = x
                end
            end
        end
    end
    return BodyPart
end

local GetClosestBodyPartC = function (character)
    local ClosestDistance
    local BodyPart
    if (character and character:GetChildren()) then
        for _,  x in next, character:GetChildren() do
            local IsOnScreen = camera:WorldToScreenPoint(character.HumanoidRootPart.Position)
            if FilterObjs(x) and IsOnScreen then
                local Distance = (Vector2.new(IsOnScreen.X, IsOnScreen.Y) - Vector2.new(mouse.X, mouse.Y)).Magnitude
                if Distance <= FovToUse * 3 and Settings.AimAssist.ClosestPart.Partz:string.find(x.Name) then
                    ClosestDistance = Distance
                    BodyPart = x
                end
            end
        end
    end
    return BodyPart
end

local GetnearestCursorPoint = function()
    local NearestPositionsxyz = nil
    local NearestPositionX = nil
    local NearestPositionY = nil
    local NearestPositionZ = nil
    if Prey ~= nil then
        if Prey.Character:FindFirstChild(PartToUse) then
            local Hit, Half = Mouse.Hit.p, Prey.Character:FindFirstChild(PartToUse).Size * 0.5
            local Transform = Prey.Character[PartToUse].CFrame:ToObjectSpace(Hit)
            NearestPositionX = Prey.Character[PartToUse].CFrame * Vector3.new(math.clamp(Transform.X, -Half.X, Half.X))
            NearestPositionY = Prey.Character[PartToUse].CFrame * Vector3.new(math.clamp(Transform.Y, -Half.Y, Half.Y))
            NearestPositionZ = Prey.Character[PartToUse].CFrame * Vector3.new(math.clamp(Transform.Z, -Half.Z, Half.Z))
            NearestPositionsxyz = CFrame.new(NearestPositionX, NearestPositionY, NearestPositionZ)
        end
    end
    return NearestPositionsxyz
end


local calculateVelocity = function(part)
    local prevPosition = part.Position
    local prevTime = tick()

    local function updateVelocity()
        local currentTime = tick()
        local deltaTime = currentTime - prevTime
        local currentPosition = part.Position
        local deltaPosition = currentPosition - prevPosition
        local velocity = deltaPosition / deltaTime

        part.Velocity = velocity
        prevPosition = currentPosition
        prevTime = currentTime
    end

    return updateVelocity
end

local ChangeVel = function(X, Y, Z, Target, Done)
    local hrpv = Target.Character.HumanoidRootPart.Velocity
    local ChangedVel = false
    local hrpx = hrpv.X
    local hrpy = hrpv.Y
    local hrpz = hrpv.Z
    if Done == false then
        hrpv = Vector3.new(X, Y, Z)
        Done = true
    end
end


local CheckIfJumping = function(Character)
    local Humanoid = Character.Humanoid
    local IsJumping = false
    if Humanoid.State = Falling then
        IsJumping = true
    else
        IsJumping = false
    end
    return IsJumping 
end



local function GetRandomShake()
    local shakeX = math.random(-Settings.AimAssist.ShakePower.X, Settings.AimAssist.ShakePower.X)
    local shakeY = math.random(-Settings.AimAssist.ShakePower.Y, Settings.AimAssist.ShakePower.Y)
    local shakeZ = math.random(-Settings.AimAssist.ShakePower.Z, Settings.AimAssist.ShakePower.Z)
  
    return Vector3.new(shakeX, shakeY, shakeZ)
end

local checkTargetHealth = function(target)
    local targeth = target.Character.Humanoid
    local dead = false
    if targeth.Health < 2 then
        local dead = true
    end
    return dead
end

local GetDistanceFromPlr = function(You, Target)
    local Distance = nil
    local GotDistance = false
    local YouHRP = Client.Character.HumanoidRootPart.Position
    local TargetHRP = Target.Character.HumanoidRootPart.Position
    if Target and You then
        DistanceFromEachOther = (YouHRP - TargetHRP).Magnitude
        if DistanceFromEachOther ~= nil then
            GotDistance = true
            Distance = DistanceFromEachOther
        end
    end
    return Distance
end

local CalculateHitChance = function(Enabled, Chance)
    local ChanceToHit = nil
    local GonnaHit = false
    local Max = 100
    if Enabled == true then
        if Chance >= Max then
            ChanceToHit = 100
            GonnaHit = true
        elseif Chance >= 80 and Chance < Max then
            ChanceToHit = math.random(1, 20)
            if ChanceToHit < 16 then
                GonnaHit = true
            else
                GonnaHit = false
            end
        elseif Chance >= 60 and Chance < Max and Chance < 80 then
            ChanceToHit = math.random(1, 40)
            if ChanceToHit < 25 then
                GonnaHit = true
            else
                GonnaHit = false
            end
        elseif Chance >= 40 and Chance < Max and Chance < 60 and Chance < 80 then
            ChanceToHit = math.random(1, 60)
            if ChanceToHit < 40 then
                GonnaHit = true
            else
                GonnaHit = false
            end
        elseif Chance >= 20 and Chance < Max and Chance < 60 and Chance < 80 and Chance < 40 then
            ChanceToHit = math.random(1, 80)
            if ChanceToHit < 60 then
                GonnaHit = true
            else
                GonnaHit = false
            end
        elseif Chance < 20 then
            GonnaHit = false
        end
    end
    return GonnaHit
end


task.spawn(function ()
    while task.wait() do
        if Prey and Prey:FindFirstChild("Character") then
            if Settings.Silent.Enabled and Settings.Silent.ClosestPart == true then
                PartToUse = tostring(GetClosestBodyPart(Prey.Character))
            end
            if Settings.Silent.Enabled and Settings.Silent.ClosestPoint == true and Settings.Silent.Part ~= nil then
                PointToAim = GetnearestCursorPoint()
                print(PointToAimP)
            end    
            if Settings.Silent.DisableOnDeath == true then
                if Prey.Character.Humanoid.Health < 2 then
                    pcall(function()
                        Prey = nil
                    end)
                end
            end
        end
    end
end)


RS.RenderStepped:Connect(function()
    -- SILENT AIM FUNCTIONS
    if Prey ~= nil then
        DistanceFromTheNigga = GetDistanceFromPlr(Client, Prey)
        if Settings.Silent.DisabledOnDeath == true then
            local health = checkTargetHealth(Prey)
            if health = true then
                Prey = nil
            end
        end
        if Settings.Silent.UseHitChance == true then
            GonnaHit = CalculateHitChance(true, Settings.Silent.HitChance)
        end
    end
    -- // RESOLVER FUNCTIONS 
    if Settings.Prey_Resolving.Velocity_ReCalculator == true then
        if Prey ~= nil or CPlayer ~= nil then 
            wait(Settings.Prey_Resolving.ReCalculation_Interval)
            calculateVelocity(Prey.Character.PartToUse)
        end
    end
    if Settings.Prey_Resolving.Velocity_Changer.Enabled == true then
        if Prey or CPlayer then
            local Target1 = Prey
            local Target2 = CPlayer
            local Sucsess = ChangeVel(Settings.Prey_Resolving.Velocity_Changer.X, Settings.Prey_Resolving.Velocity_Changer.Y, Settings.Prey_Resolving.Velocity_Changer.Z, Target1, false)
            local Sucsess2 = ChangeVel(Settings.Prey_Resolving.Velocity_Changer.X, Settings.Prey_Resolving.Velocity_Changer.Y, Settings.Prey_Resolving.Velocity_Changer.Z, Target2, false)
        end
    end
    -- // Cam functions // -- 
    if Settings.AimAssist.DisabledOnDeath == true and CPlayer then
        local health = checkTargetHealth(CPlayer)
        if health = true then
            CPlayer = nil
        end
    end
    if Settings.AimAssist.DisableOnYourDeath == true and CPlayer then
        local h = Client.Character.Humanoid
        if h.Health < 2 then
            CPlayer = nil
        end
    end
    if Settings.AimAssist.CheckIfJumped == true and CPlayer then
        local IsJumping = CheckIfJumping(CPlayer.Character)
        if IsJumping == true then
            PartToUseC = Settings.AimAssist.CheckIfJumpedAimPart
        end
    end

end)

--KEYS FOR STUFF IN THE SCRIPT
Mouse.KeyDown:Connect(function(key)
    if Settings.Prey_Resolving.Use_MoveDirection_Key == true then
        if Key.KeyCode == Settings.Prey_Resolving.MoveDirection_Resolver_key then
            UseMove = not UseMove
        end
    end
    if Settings.Silent.UseKeybindToDisable == true then
        if Key.KeyCode == Settings.Silent.Keybind then
            Settings.Silent.Enabled = not Settings.Silent.Enabled
        end
    end
    if Settings.AimAssist.UseKeyBoardKey == true and Settings.AimAssist.AttachOnSilent == false then
        if Key.KeyCode = Settings.AimAssist.KeyBoardKey then
            cplayer = ClosestPlrFromMouse()
            canC = true
        end
    end
end)


 
-- GET GUN EQUIPED
local BackPack = Client.Backpack
local GunEquipped = false


for _, gunName in pairs(Guns) do
    local gun = BackPack:FindFirstChild(gunName)
    if gun then
        gun.Equipped:Connect(function()
            if gun.Name ~= "Wallet" and gun.Name ~= "[Phone]" and gun.Name ~= "Combat" and gun.Name ~= "[SprayCan]" then
                GunEquipped = true
                gunEquipedName = gun.Name
            end
        end)
        gun.Unequipped:Connect(function()
            if gun.Name ~= "Wallet" and gun.Name ~= "[Phone]" and gun.Name ~= "Combat" and gun.Name ~= "[SprayCan]" then
                if GunEquipped == true then
                    GunEquipped = false
                    gunEquipedName = nil
                end
            end
        end)
    end
end

--- TEXT COMMANDS IG---

Client.Chatted:Connect(function(Commands)
    if Settings.Commands.Enabled == true then
        local AimCommands = Settings.Commands.AimAssist
        local SilentCommands = Settings.Commands.Silent
        if Commands == Settings.Commands.Silent.Silent_Enabled then
            Settings.Silent.Enabled = not Settings.Silent.Enabled
        elseif Commands == Settings.Commands.AimAssist.AimAssist_Enabled then
            Settings.AimAssist.Enabled = not Settings.AimAssist.Enabled
        elseif Commands == SilentCommands.FovVisible then
            Settings.FOV.Silent.Visible = not Settings.FOV.Silent.Visible
        elseif Commands == AimCommands.ShowFov then
            Settings.FOV.AimAssist.Visible = Settings.FOV.AimAssist.Visible
        end
        local msg = string.split(Commands, "")
        if msg[1] == SilentCommands.Pred and msg[2] ~= nil then
            Settings.Silent.Pred = msg[2]
        elseif msg[1] == SilentCommands.FovSize and msg[2] ~= nil then
            FovToUse = msg[2]
        elseif msg[1] == AimCommands.Pred and msg[2] ~= nil then
            Settings.AimAssist.Pred = msg[2]
        elseif msg[1] == AimCommands.SmoothNess and msg[2] ~= nil then
            Settings.AimAssist.Smoothness = msg[2]
        elseif msg[1] == AimCommands.Use_ShakeValue and msg[2] ~= nil then
            Settings.AimAssist.ShakePower = msg[2]
        elseif msg[1] == AimCommands.FovSize and msg[2] ~= nil then
            Settings.FOV.AimAssist.Radius = msg[2]
        end
    end
end)

-- // SILENT AIM HOOK
local grmt = getrawmetatable(game)
local backupindex = grmt.__index
setreadonly(grmt, false)

grmt.__index = newcclosure(function(self, v)
if (Settings.Silent.Enabled and Mouse and tostring(v) == "Hit") then

    Prey = ClosestPlrFromMouse()
    local endpoint
    local move = Players[tostring(Prey)].Character.Humanoid.MoveDirection


    if Prey then
        PPred = (Vector3.new(Prey.Character.HumanoidRootPart.Velocity.X, (Prey.Character.HumanoidRootPart.Velocity.Y * 0.5), Prey.Character.HumanoidRootPart.Velocity.Z) * Settings.Silent.Pred)
        if Settings.Silent.ClosestPoint == true then
            endpoint = PointToAim + PPred
        elseif Settings.Silent.ClosestPoint == false and Settings.Silent.ClosestPart == true and Settings.Resolver.Enabled == false then
            endpoint = game.Players[tostring(Prey)].Character[PartToUse].CFrame + (game.Players[tostring(Prey)].Character[PartToUse].Velocity * Settings.Silent.Pred)
        elseif Settings.Silent.ClosestPoint == false and Settings.Silent.ClosestPart == true and UseMove == true then
            endpoint = game.Players[tostring(Prey)].Character[PartToUse].CFrame + (move * Settings.Silent.Pred)
        elseif Settings.Silent.ClosestPoint == false and Settings.Silent.ClosestPart == true and Settings.Resolver.Enabled == true then
            endpoint = PointToAim + (move * Settings.Silent.Pred)
        end
        return (tostring(v) == "Hit" and endpoint)
    end
end
return backupindex(self, v)
end)

-- // CAMLOCK
RS.Heartbeat:Connect(function()
    if Settings.AimAssist.Enabled == true then
        if canC == true then
            local Main
            local Smoothness
            local move1 = Players[tostring(Prey)].Character.Humanoid.MoveDirection
            local move2 = Players[tostring(CPlayer)].Character.Humanoid.MoveDirection
            if Settings.AimAssist.AttachOnSilent == true and Prey then
                Main = CF(Camera.CFrame.p, Prey.Character[PartToUseC].Position + (Prey.Character[PartToUseC].Position.Velocity * Settings.AimAssist.Pred))
            elseif Settings.AimAssist.AttachOnSilent == false and CPlayer then
                Main = CF(Camera.CFrame.p, CPlayer.Character[PartToUseC].Position + (CPlayer.Character[PartToUseC].Position.Velocity * Settings.AimAssist.Pred))
            elseif Settings.AimAssist.AttachOnSilent == true and Prey and UseMove then
                Main = CF(Camera.CFrame.p, Prey.Character[PartToUse].CFrame + (move1 * Settings.AimAssist.Pred))
            elseif Settings.AimAssist.AttachOnSilent == true and CPlayer and UseMove then
                Main = CF(Camera.CFrame.p, CPlayer.Character[PartToUse].CFrame + (move2 * Settings.AimAssist.Pred))
            end
            Camera.CFrame = Camera.CFrame:Lerp(Main, Settings.AimAssist.Smoothness, Settings.AimAssist.Easing, Enum.EasingDirection.InOut)
        end
    end
end)
